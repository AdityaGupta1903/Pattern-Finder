{
  "manifest_version": 3,
  "name": "Path Finder",
  "version": "1.0.0",
  "description": "A simple React app as a Chrome extension",
  "permissions": ["scripting", "activeTab","tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_idle"
    }
  ]
}

